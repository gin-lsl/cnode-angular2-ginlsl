div {
  height: 50px;
  /*border: 1px solid #ccc;*/
  line-height: 50px;
}

div:hover {
  background-color: #ECECEC;
}

a {
  display: inline-block;
  width: 100%;
  height: 100%;
  text-decoration: none;
  font-family: "微软雅黑", serif;
  font-size: 20px;
  color: black;
}

a.active {
  background-color: #F7F7F7;
}

.menu_item_icon {
  font-family: 'Segoe MDL2 Assets';
  text-align: center;
  width: 50px;
  height: 50px;
  float: left;
  font-size: 25px;
}

.collapse_pane {
  width: 50px;
  overflow: hidden;
}

.visibility_pane {
  width: 100%;
}
